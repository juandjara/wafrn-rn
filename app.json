{
  "expo": {
    "newArchEnabled": true,
    "name": "Wafrn",
    "slug": "wafrn-rn",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_w.png",
    "scheme": "wafrn",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/wafrn-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#151718"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.djara.wafrn-rn"
    },
    "android": {
      "androidStatusBar": {
        "translucent": false,
        "backgroundColor": "#151718",
        "barStyle": "light-content"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_w.png",
        "backgroundColor": "#151718"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "dev.djara.wafrn_rn",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "app.wafrn.net"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8453855c-b400-4d59-bd72-0ebfa1a95eb2"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8453855c-b400-4d59-bd72-0ebfa1a95eb2"
    }
  }
}
