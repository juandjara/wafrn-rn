{
  "cli": {
    "version": ">= 11.0.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "environment": "development",
      "android": {
        "gradleCommand": ":app:assembleDevelopmentDebug"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "preview",
      "environment": "production",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleProductionRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}