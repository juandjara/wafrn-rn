    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true
      }
    },
    "autolinking": {
      "android": {
        "buildFromSource": []
      }
    }
  },
  "packageManager": "npm@10.9.0",
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }
}