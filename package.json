    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-unified-push": "0.3.4",
    "expo-updates": "~0.28.13",
    "expo-video": "~2.1.9",
    "houseform": "^1.13.0",
    "html-crush": "^4.2.0",
    "htmlparser2": "^9.1.0",
    "jsdom-jscore-rn": "^0.1.9",
    "nativewind": "^4.1.23",
    "qr": "^0.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-awesome-gallery": "^0.4.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-more-controlled-mentions": "3.0.1",
    "react-native-pager-view": "6.8.1",
    "react-native-popup-menu": "0.17.0",
