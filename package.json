{
  "name": "wafrn-rn",
  "main": "expo-router/entry",
  "version": "1.4.6",
  "scripts": {
    "update:preview_ci": "npx eas-cli update --channel preview --auto --non-interactive --environment preview",
    "update:preview": "npx eas-cli update --channel preview --auto --environment preview",
    "build:dev:ios": "NODE_ENV=development npx eas-cli build --profile development --local --platform ios",
    "build:dev:android": "bash ./scripts/android_build.sh dev",
    "build:prod:ios": "cd ios && fastlane beta",
    "build:prod:android": "bash ./scripts/android_build.sh prod",
    "setup:dev": "NODE_ENV=development expo prebuild",
    "setup:prod": "NODE_ENV=production expo prebuild && bash ./scripts/sed_gradle_version.sh",
    "start": "NODE_ENV=development expo start",
    "android": "NODE_ENV=development expo run:android --app-id dev.djara.wafrn_rn.dev",
    "ios": "NODE_ENV=development expo run:ios",
    "lint": "expo lint",
    "test": "node scripts/checkEnvironment.ts",
    "preversion": "npm test",
    "version": "npm run setup:prod && git add -A .",
    "postversion": "git push && git push origin $(git describe --tags)"
  },
  "dependencies": {
    "@backpackapp-io/react-native-toast": "^0.13.0",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^4.1.3",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.0",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.36",
    "@shopify/flash-list": "1.7.6",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "@tanstack/react-query": "^5.77.2",
    "@types/showdown": "^2.0.6",
    "@xstate/store": "^2.6.2",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "emoji-datasource": "^15.1.2",
    "expo": "^53.0.9",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-device": "~7.1.4",
    "expo-document-picker": "~13.1.5",
    "expo-file-system": "~18.1.10",
    "expo-font": "~13.3.1",
    "expo-image": "~2.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.5",
    "expo-media-library": "~17.1.6",
    "expo-notifications": "0.29.14",
